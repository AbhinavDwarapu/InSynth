{
  "extends": [
    "airbnb",
    "next/core-web-vitals",
    "plugin:cypress/recommended"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, {
      "extensions": [".js", ".jsx"] 
    }],
    "react/jsx-no-bind" : [0, {
      "ignoreDOMComponents" : true
    }],
    "jsx-a11y/anchor-is-valid" : [
      0
    ]
  }
}
